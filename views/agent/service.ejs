                                <form class="mb-3" action="<%=q%>/accept" method="post">
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingFirstName" type="text" placeholder="Dept name" name="department_name" />

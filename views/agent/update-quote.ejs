        <section id="news">
            <div class="container">
                <div class="row">
                    <h2 class="text-light text-center my-5">Quotes</h2>
                </div>
                <div class="row justify-content-center">
                    <div class="col-md-5 col-xl-5 pe-xxl-0">
                        <div class="card card-bg shadow hero-header-form">
                            <div class="card-body p-4 p-xl-6">
                                <h2 class="text-100 text-center">Update Quote</h2>
                                
                                <form class="mb-3" action="update/<%= quotes.quote_id %>" method="post">
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingQuoteId" type="text" placeholder="Quote ID" name="quote_id" value="<%= quotes.quote_id %>" />
                                        <label for="floatingQuoteId">Quote ID</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingApplicationId" type="text" placeholder="Application ID" name="application_id" value="<%= quotes.application_id %>" />
                                        <label for="floatingApplicationId">Application ID</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingCustId" type="text" placeholder="Customer ID" name="cust_id" value="<%= quotes.cust_id %>" />
                                        <label for="floatingCustId">Customer ID</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingIssueDate" type="text" placeholder="Issue Date" name="issue_date" value="<%= quotes.issue_date.toISOString().slice(0,10) %>" />
                                        <label for="floatingIssueDate">Issue Date</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingValidFromDate" type="text" placeholder="Valid From Date" name="valid_from_date" value="<%= quotes.valid_from_date.toISOString().slice(0,10) %>" />
                                        <label for="floatingValidFromDate">Valid From Date</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingValidTillDate" type="text" placeholder="Valid Till Date" name="valid_till_date" value="<%= quotes.valid_till_date.toISOString().slice(0,10) %>" />
                                        <label for="floatingValidTillDate">Valid Till Date</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingDescription" type="text" placeholder="Description" name="description" value="<%= quotes.description %>" />
                                        <label for="floatingDescription">Description</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingProductId" type="text" placeholder="Product ID" name="product_id" value="<%= quotes.product_id %>" />
                                        <label for="floatingProductId">Product ID</label>
                                    </div>
                                    <div class="form-floating mb-3">
                                        <input class="form-control input-box form-ensurance-header-control" id="floatingCoverageLevel" type="text" placeholder="Coverage Level" name="coverage_level" value="<%= quotes.coverage_level %>" />
                                        <label for="floatingCoverageLevel">Coverage Level</label>
                                    </div>
                                    <div class="col-12 d-grid">
                                        <button class="btn btn-primary rounded-pill" type="submit">Submit</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                
                
            </div>  
            <!-- end of .container-->
        </section>
        <!-- <section> close ============================-->
        <!-- ============================================-->



